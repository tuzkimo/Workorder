<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.3//EN"
        "http://struts.apache.org/dtds/xwork-validator-1.0.3.dtd">

<validators>

	<field name="workorder.ordername">
		<field-validator type="requiredstring">
			<message>工单名称不能为空</message>
		</field-validator>
	</field>

	<field name="workorder.executor">
		<field-validator type="requiredstring">
			<message>执行者不能为空</message>
		</field-validator>
	</field>
	
	<field name="workorder.description">
		<field-validator type="requiredstring">
			<message>描述不能为空</message>
		</field-validator>
	</field>
	
</validators>